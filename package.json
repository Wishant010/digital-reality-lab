{
  "name": "wishantbhajan",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://wishantbhajan.nl",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:fast": "vite build --mode development",
    "build:analyze": "npm run build && npx vite-bundle-analyzer",
    "lint": "eslint . --fix",
    "preview": "vite preview --host",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite .turbo",
    "clean:cache": "npm run clean && npm cache clean --force",
    "analyze": "npx vite-bundle-analyzer",
    "performance": "npm run build && npm run preview",
    "predeploy": "npm run build",
    "deploy": "npm run build && type nul > dist\\CNAME && echo wishantbhajan.nl > dist\\CNAME && gh-pages -d dist --dotfiles",
    "deploy:ci": "npm run build && touch dist/CNAME && echo 'wishantbhajan.nl' > dist/CNAME"
  },
  "dependencies": {
    "@react-three/drei": "^10.3.0",
    "@react-three/fiber": "^9.1.2",
    "framer-motion": "^11.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.28.0",
    "three": "^0.177.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@types/three": "^0.177.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cssnano": "^7.0.7",
    "eslint": "^9.30.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.2.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^5.4.11",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.2.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wishant010/wishantbhajan.git"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "wishantbhajan",
    "web-development",
    "three.js",
    "framer-motion"
  ],
  "author": "Wishant Bhajan",
  "license": "MIT",
  "description": "High-performance web application for wishantbhajan.nl using React, Three.js, and modern deployment."
}